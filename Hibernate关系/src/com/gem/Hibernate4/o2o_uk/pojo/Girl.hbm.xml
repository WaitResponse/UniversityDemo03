<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >


<hibernate-mapping package="com.gem.Hibernate4.o2o_uk.pojo">
	<class name="Girl" table="t_girl">
		<id name="gid">
			<generator class="native"/>
